<h1>Listing food_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @food_types.each do |food_type| %>
  <tr>
    <td><%= food_type.name %></td>
    <td><%= link_to 'Show', food_type %></td>
    <td><%= link_to 'Edit', edit_food_type_path(food_type) %></td>
    <td><%= link_to 'Destroy', food_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

